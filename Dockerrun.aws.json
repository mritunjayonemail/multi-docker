{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions":[
		{	
			"name": "client",
			"image": "mritunjayonemail/multi-client",
			"hostname": "client",
			"essential": false
		},
		{	
			"name": "server",
			"image": "mritunjayonemail/multi-server",
			"hostname": "api",
			"essential": false
		},
		{	
			"name": "worker",
			"image": "mritunjayonemail/multi-worker",
			"hostname": "worker",
			"essential": false
		},
		{	
			"name": "nginx",
			"image": "mritunjayonemail/multi-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["client", "server"]

		},
		
	]
	
}